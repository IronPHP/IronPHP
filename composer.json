{
    "name": "ironphp/ironphp",
    "description": "The IronPHP Framework",
    "homepage": "https://github.com/ironphp/ironphp",
    "type": "project",
    "keywords": [
        "php",
        "mvc",
        "framework",
        "ironphp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "GaurangKumar Parmar",
            "email": "gaurangkumarp@gmail.com",
            "homepage": "https://twitter.com/gaurangkumarp",
            "role": "Founder and Developer"
        }
    ],
    "support": {
        "email": "gaurangkumarp@gmail.com",
        "homepage": "https://www.facebook.com/IronPHP-Framwork-325690624644002",
        "issues": "https://github.com/ironphp/ironphp/issues",
        "source": "https://github.com/ironphp/ironphp"
    },
    "require": {
        "php": ">=5.5"
    },
    "autoload": {
        "psr-4": {
            "Friday\\": "src/Friday/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Friday\\": "src/Friday/",
            "App\\": "app/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php jarvis key"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
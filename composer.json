    },
    "require": {
        "php": ">=5.5",
        "psr/log": "1.1.0",

        "doctrine/inflector": "^1.1",
        "dragonmantank/cron-expression": "^2.0",
        "egulias/email-validator": "^2.0",
        "erusev/parsedown": "^1.7",
        "league/flysystem": "^1.0.8",
        "monolog/monolog": "^1.12",
        "nesbot/carbon": "^1.26.3 || ^2.0",
        "opis/closure": "^3.1",
        "psr/container": "^1.0",
        "psr/simple-cache": "^1.0",
        "ramsey/uuid": "^3.7",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/console": "^4.2",
        "symfony/debug": "^4.2",
        "symfony/finder": "^4.2",
        "symfony/http-foundation": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/process": "^4.2",
        "symfony/routing": "^4.2",
        "symfony/var-dumper": "^4.2",
        "tijsverkoyen/css-to-inline-styles": "^2.2.1",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Friday\\": "src/Friday/"
        },
        "files": [
            "src/Friday/Helper/Function.php",
            "src/Friday/Helper/Constant.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
